          "name": "POST /auth/login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", () => pm.response.to.have.status(200));",
                  "pm.test(\"Response time < 1000ms\", () => pm.expect(pm.response.responseTime).to.be.below(1000));",
                  "pm.test(\"Response has token\", () => {",
                  "  const json = pm.response.json();",
                  "  pm.expect(json).to.have.property(\"accessToken\");",
                  "  pm.expect(json.accessToken).to.be.a(\"string\");",
                  "  pm.collectionVariables.set(\"token\", json.accessToken);",
                  "});",
                  "pm.test(\"Response has user info\", () => {",
                  "  const json = pm.response.json();",
                  "  pm.expect(json).to.have.property(\"id\");",
                  "  pm.expect(json).to.have.property(\"username\");",
                  "  pm.expect(json).to.have.property(\"email\");",
                  "});",
                  "",
                  "// JSON Schema Validation",
                  "const schema = {",
                  "  type: \"object\",",
                  "  required: [\"accessToken\", \"id\", \"username\", \"email\"],",
                  "  properties: {",
                  "    accessToken: { type: \"string\" },",
                  "    id: { type: \"number\" },",
                  "    username: { type: \"string\" },",
                  "    email: { type: \"string\" }",
                  "  }",
                  "};",
                  "pm.test(\"Schema is valid\", () => {",
                  "  pm.response.to.have.jsonSchema(schema);",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
